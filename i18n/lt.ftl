# -- Other captions
figure = pav.
table  = lentelė
proof  = Įrodymas

# -- TOC-like headers
tableofcontents = Turinys
listoffigures = Iliustracijų sąrašas
listoftables = Lentelių sąrašas

# -- Other usual headers
abstract = Santrauka
bibliography = Literatūra
glossary = Glosarijus
index = Rodyklė
preface = Pratarmė
references = Literatūra

# -- Miscellaneous
page = puslapis
see = žiūrėk
